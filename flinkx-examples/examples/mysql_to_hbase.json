{
  "job": {
    "setting": {
      "speed": {
        "channel": 1
      },
      "errorLimit": {
        "record": 0,
        "percentage": 0.02
      }
    },
    "content": [
      {
        "reader": {
          "name": "mysqlreader",
          "parameter": {
            "username": "dtstack",
            "password": "abc123",
            "column": [
              "col1",
              "col2"
            ],
            "splitPk": "col1",
            "connection": [
              {
                "table": [
                  "tb2"
                ],
                "jdbcUrl": [
                  "jdbc:mysql://172.16.8.104:3306/test?useCursorFetch=true"
                ]
              }
            ]
          }
        },
        "writer": {
          "name": "hbasewriter",
          "parameter": {
            "hbaseConfig": {
              "hbase.zookeeper.property.clientPort": "2181",
              "hbase.rootdir": "hdfs://ns1/hbase",
              "hbase.cluster.distributed": "true",
              "hbase.zookeeper.quorum": "node01,node02,node03",
              "zookeeper.znode.parent": "/hbase"
            },
            "table": "tb1",
            "rowkeyColumn": "col1#col2",
            "column": [
              {
                "name": "cf1:id",
                "type": "string"
              },
              {
                "name": "cf1:vv",
                "type": "string"
              }
            ]
          }
        }
      }
    ]
  }
}
